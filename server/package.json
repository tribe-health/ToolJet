    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0"
  },
  "engines": {
    "node": ">=16.13.1",
    "npm": "<=8.6.0"
  }
}
